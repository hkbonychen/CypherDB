module or1200_encryption(
			 clk,
			 rst,
			 dataIn1,
			 dataOut1,
			 dataIn2,
			 dataOut2,
			 stall,
			 unstall);

   input clk;
   input rst;
   input dataIn1;
   input dataOut2;

   output dataOut1;
   output dataOut2;
   output stall;
   output unstall;

   
   